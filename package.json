{
  "homepage": "https://albertofp.github.io/nutrition-tracker-react/",
  "name": "nutrition-tracker-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "tailwind": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "tailwind:watch": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "gen-types": "npx supabase gen types typescript --linked > types/supabase.ts",
    "test": "vitest --coverage",
    "cypress": "cypress open"
  },
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@hookform/resolvers": "^3.0.1",
    "@mantine/carousel": "^6.0.9",
    "@mantine/core": "^6.0.9",
    "@mantine/hooks": "^6.0.9",
    "@mantine/notifications": "^6.0.9",
    "@supabase/auth-ui-react": "^0.3.5",
    "@supabase/auth-ui-shared": "^0.1.3",
    "@supabase/supabase-js": "^2.20.0",
    "@tanstack/router": "^0.0.1-beta.84",
    "cypress": "^12.14.0",
    "lucide-react": "^0.187.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "supabase": "^1.50.4",
    "unsplash-js": "^7.0.15",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "cssnano": "^6.0.1",
    "gh-pages": "^5.0.0",
    "jsdom": "^21.1.1",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vitest": "^0.30.1"
  }
}
